section .data
    a db 2
    b db 5
    c db 13

section .text
    char db 0
    global _start

% macro suma 1
    mov al, %1   
    add al, %2
    add al, %3
_start:
    ; Sumar dos números distintos
    
    suma a,b,c

    ; Escribir carácter en consola
    mov eax, 4         ; syscall write
    mov ebx, 1         ; stdout
    mov ecx, char
    mov edx, 1         ; 1 byte
    int 0x80

    ; Salir
    mov eax, 1         ; syscall exit
    xor ebx, ebx       ; código de salida 0
    int 0x80
